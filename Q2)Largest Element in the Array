Problem Statement:
Given an array 'arr' of size 'n' find the largest element in the array

import java.util.* ;
import java.io.*; 

public class Solution {

    static int largestElement(int[] arr, int n) {
        int largest= Integer.MIN_VALUE;
        for(int i=0;i<arr.length;i++){
        if(largest<arr[i]){
            largest=arr[i];
        }
        }
    return largest;
    }
}
